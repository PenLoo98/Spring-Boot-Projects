{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">email</th>
        <th scope="col">password</th>
    </tr>
    </thead>
    <tbody>
    {{#foundMember}}
        <tr>
            <th>{{id}}</th>
            <th>{{email}}</th>
            <th>{{password}}</th>
        </tr>
    {{/foundMember}}
    </tbody>
</table>
<a href="/members/{{foundMember.id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/members/{{foundMember.id}}/delete" class="btn btn-danger">삭제하기</a>
<a href="/members">Go to member list</a>

{{>layouts/footer}}