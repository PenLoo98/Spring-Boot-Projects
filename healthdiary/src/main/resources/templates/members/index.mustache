{{>layouts/header}}
<h1>이용자 정보</h1>
<a href="/members/signup">
    <button class="btn btn-primary">
        회원가입
    </button>
</a>

<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">email</th>
        <th scope="col">password</th>
    </tr>
    </thead>
    <tbody>
    {{#memberList}}
        <tr>
            <th>{{id}}</th>
            <th><a href="/members/{{id}}">{{email}}</a></th>
            <th>{{password}}</th>
        </tr>
    {{/memberList}}
    </tbody>
</table>

{{>layouts/footer}}